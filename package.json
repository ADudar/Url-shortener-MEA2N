{
  "name": "url-shortener",
  "version": "0.0.0",
  "license": "MIT",
  "main": "server/app.js",
  "angular-cli": {},
  "scripts": {
    "start": "concurrently --kill-others  \"ng serve --aot=false --proxy-config proxy.conf.json\"  \"npm run server\" ",
    "test:karma": "ng test --watch=true",
    "test:mocha": "npm run server:build && mocha ./dist/server/tests --recursive",
    "lint": "     concurrently \"npm run lint:server\" \"npm run lint:client\"   ",
    "lint:server": "tslint \"server/**/*.ts",
    "lint:client": "tslint \"src/**/*.ts",
    "build": "ng build --prod --sm=false --aot=false --output-path=dist/client && npm run server:build",
    "client": "ng serve --aot=false --proxy-config proxy.conf.json",
    "server": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/body-parser": "^0.0.34",
    "@types/chai": "^3.4.35",
    "@types/chai-http": "0.0.30",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.0.32",
    "@types/express-jwt": "0.0.34",
    "@types/jsonwebtoken": "^7.1.31",
    "@types/jwt-decode": "^1.4.28",
    "@types/mocha": "^2.2.39",
    "@types/mongodb": "^2.1.40",
    "@types/mongoose": "^4.7.6",
    "@types/node": "^7.0.5",
    "angular2-jwt": "^0.1.23",
    "bcryptjs": "^2.4.0",
    "body-parser": "~1.16.1",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "express": "~4.14.0",
    "express-jwt": "^5.1.0",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^4.7.7",
    "ng2-pagination": "^2.0.0",
    "ngx-clipboard": "^3.0.5",
    "q": "^1.4.1",
    "rxjs": "^5.0.1",
    "tether": "^1.4.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.32.3",
    "@angular/compiler-cli": "^2.4.0",
    "@types/chai-http": "0.0.30",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.3.0",
    "jasmine-spec-reporter": "^3.2.0",
    "jwt-decode": "^2.1.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.0.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.0",
    "ts-node": "2.1.0",
    "tslint": "^4.3.0",
    "typescript": "~2.1.6"
  }
}
